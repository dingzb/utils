<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="OFF">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %c %M%n%p: %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<root level="debug">
			<appender-ref ref="Console" />
		</root>
	</Loggers>
</Configuration>  